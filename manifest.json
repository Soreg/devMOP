{
  "manifest_version": 2,
  "name": "DevMOP",
  "version": "1.0.1",
  "description": "turtles-team-1 extension - CHANGE",
  "icons": {
     "64": "icon.png"
  },
  "browser_action": {
     "default_icon": "icon.png"
  },
  "background": {
     "persistent": false,
     "scripts": ["js/background.js"]
  },
  "permissions": ["tabs"],
  "web_accessible_resources": ["*.ttf" ],
  "chrome_url_overrides": {
     "newtab": "test.html"
  },
  "content_scripts": [
  {
     "matches": ["http://*/*", "https://*/*"],
     "js": ["/js/app.js"]
  }
  ]
}
